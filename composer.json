{
    "name": "viloveul/cache",
    "description": "Viloveul cache.",
    "type": "component",
    "license": "MIT",
    "authors": [{
        "name": "Fajrul Akbar Zuhdi",
        "email": "fajrulaz@gmail.com"
    }],
    "keywords": ["component", "zafex", "viloveul", "fajrulaz", "cache"],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "~7.0",
        "psr/simple-cache": "^1.0"
    },
    "require-dev": {
        "codeception/base": "^2.5",
        "viloveul/support": "v1.*"
    },
    "suggest": {
        "ext-redis": "For using redis adapter",
	"ext-apcu": "For use apcu cache adapter"
    },
    "autoload": {
        "psr-4": {
            "Viloveul\\Cache\\": "src/"
        }
    }
}
